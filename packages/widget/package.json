{
  "name": "@kanbn/feedback-widget",
  "version": "0.1.0",
  "description": "Embeddable feedback widget for Kan â€” screenshot, annotate, and submit bug reports",
  "type": "module",
  "main": "dist/kan-widget.umd.js",
